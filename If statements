#include <stdio.h>
#include <stdlib.h>


int max(int num1, int num2)
{
    int result;
    if(num1 > num2)
    {
        result = num1;
    }
    else
    {
        result = num2;
    }
    
    return result;
}


int main()
{
    printf("The bigger number of the two is %d", max(81,79));
	return 0;
}
