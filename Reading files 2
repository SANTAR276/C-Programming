#include <stdio.h>

int main(){

    FILE *file = fopen("test.txt", "r");

    char buffer[255];

    if(file == NULL){
        printf("File does not exist!");
    }
    else{
        while(fgets(buffer, 255, file) != NULL){
            printf("%s", buffer);
        }
    }

    fclose(file);
    
    return 0;
}
