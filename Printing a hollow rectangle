#include <stdio.h>

int main(){

    int x, y;

    printf("Enter the height of the rectangle: ");
    scanf("%d", &x);

    printf("Enter the width of the rectangle: ");
    scanf("%d", &y);

    for(int i = 1; i <= x; i++){
        for(int j = 1; j <= y; j++){
            if(i == 1 || i == x){
                printf("*");
            }
            else{
                if(j == 1 || j == y){
                    printf("*");
                }
                else{
                    printf(" "); 
                }
            }
        }
        printf("\n");
    }
    return 0;
}
