#include <stdio.h>
#include <stdlib.h>


int main()
{
    char grade;
    
    printf("What grade did you get??\n");
    scanf("%c", &grade);
    
    switch(grade)
    {
        case 'A':
            printf("You did great!");
            break;
        case 'B':
            printf("You did pretty well");
            break;
        case 'C':
            printf("You did okay");
            break;
        case 'D':
            printf("You did pretty bad");
            break;
        case 'F':
            printf("You failed");
            break;
        default :
            printf("Invalid grade");
        
    }
    
    
	return 0;
}
