#include <stdio.h>

int main(){

    for(int i = 1; i <=20; i++){

        if(i ==10){
            continue;
        }
        printf("%d\n", i);          // A continue will skip the loop for that specific condition and continue with the loop after that
    }

    for(int j = 1; j <= 20; j++){
        
        if(j == 10){
            break;
        }
        printf("%d\n", j);          // A break will exit the loop entirely
    }
    return 0;
}
