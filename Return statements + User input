#include <stdio.h>
#include <stdbool.h>

void birthday(char name[], int age){
    printf("Happy birthday %s!\n", name);
    printf("You are now %d years old!", age);
}

int main()
{
    char name[25];
    int age;

    printf("What is your name?");
    scanf("%s", name);

    printf("How old are you?");
    scanf("%d", &age);

    birthday(name, age);
	
    return 0;
}
