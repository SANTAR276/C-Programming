#include <stdio.h>

int main(){

    int nums[2][3];

    for(int i = 0; i < 2; i++){
        for(int j = 0; j < 3; j++){
            printf("Enter a number: ");
            scanf("%d", &nums[i][j]);
        }
    }

    for(int a = 0; a < 2; a++){
        for(int b= 0; b < 3; b++){
            printf("%d\t", nums[a][b]);
        }
        printf("\n");
    }
    return 0;
}
